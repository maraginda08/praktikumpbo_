package praktikum5;
import java.util.Scanner;

public class Test {
	public static void main(String []args) {
		String nama;
		Scanner input = new Scanner(System.in);
		
		System.out.print("Masukkan nama kucingmu : ");
		nama = input.next();
		
		Cat kucingsaya = new Cat();
		kucingsaya.identitas(nama);
		kucingsaya.eat();
	}
}
